class Solution {
public:
    int minSteps(string s, string t) {
        map<char,int>count;
        int res = 0;
        for(char& c : s) count[c]++;
        for(char&c : t) count[c]--;
        for(auto& it : count){
            if(it.second>0) res+= it.second;
        }
        return res;
    }
};
