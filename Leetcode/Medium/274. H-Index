//trivial
class Solution {
public:
    int hIndex(vector<int>& citations) {
        sort(citations.begin(),citations.end());
        int n = citations.size();
        int res = 0;
        for(int R = n-1; R>=0; R--){
            if(citations[R]>=n-R) res = n-R;
        }
        return res;
    }
};
