class Solution {
public:
    int divisibleTripletCount(vector<int>& nums, int d) {
        int count = 0;
        for(int i = 0; i<nums.size(); i++){
            for(int j = i+1; j<nums.size(); j++){
                for(int k = j+1; k<nums.size(); k++){
                    if((nums[i]%d+nums[j]%d+nums[k]%d)%d==0) count++;
                }
            }
        }
        return count;
    }
};
