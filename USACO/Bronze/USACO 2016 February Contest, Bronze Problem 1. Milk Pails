//somewhat similar to leetcode coin change solution
#include <bits/stdc++.h>
using namespace std;
void setIO(string s) {
	freopen((s + ".in").c_str(), "r", stdin);
	freopen((s + ".out").c_str(), "w", stdout);
}
int main() {
	setIO("pails");
    ios::sync_with_stdio(0);
    cin.tie(0);
	int x,y,m;
	cin>>x>>y>>m;
	vector<int>poss(m+1);
	poss[x]++;
	poss[y]++;
	for(int i = 1; i<poss.size(); i++){
		if(poss[i]!=0){
			for(int j = 1; j<poss.size(); j++){
				if(poss[j]==0) continue;
				if(i+j<=m){
					poss[i+j] = 1;
				}
			}
		}

	}
	int res = INT_MAX;
	for(int i = 0; i<poss.size(); i++){
		if(poss[i]!=0) res = i;
	}
	cout<<res<<endl;
    return 0;
}
