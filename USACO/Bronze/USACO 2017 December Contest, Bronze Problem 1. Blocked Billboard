#include <bits/stdc++.h>
using namespace std;
 void setIO(string s) {
	freopen((s + ".in").c_str(), "r", stdin);
	freopen((s + ".out").c_str(), "w", stdout);
}

int main(){
	setIO("billboard");
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
	int r1x1,r1y1,r1x2,r1y2;
	cin>>r1x1>>r1y1>>r1x2>>r1y2;
	int r2x1,r2y1,r2x2,r2y2;
	cin>>r2x1>>r2y1>>r2x2>>r2y2;
	int tx1,ty1,tx2,ty2;
	cin>>tx1>>ty1>>tx2>>ty2;
	int area = (r1x2-r1x1) * (r1y2-r1y1) + ((r2x2-r2x1) * (r2y2-r2y1));
    int overlap1 = max(0, min(tx2, r1x2) - max(tx1, r1x1)) *
                   max(0, min(ty2, r1y2) - max(ty1, r1y1));

    int overlap2 = max(0, min(tx2, r2x2) - max(tx1, r2x1)) *
                   max(0, min(ty2, r2y2) - max(ty1, r2y1));

    area -= overlap1;
    area -= overlap2;
	cout<<area;
	return 0;
}
