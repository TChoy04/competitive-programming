#include <bits/stdc++.h>
using namespace std;
 
/*

*/
int main(){
	ios::sync_with_stdio(0);
	int tc;
	cin>>tc;
	while(tc--){
		string num;
		cin>>num;
		int flag = 0;
		int zc = 0;
		for(int i = num.size()-1; i>=0;i--){
			if(num[i]!='0'){
				flag = 1;
			}else if(flag){
				zc++;
			}
		}
		cout<<num.size()-zc-1<<"\n";
	}
	return 0;
}
